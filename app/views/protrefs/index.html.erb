<% @orange_right="List of the reference protocols available" %>
<% @origine = "index" %>

<%= render :partial => "headfoot", :locals => { :origin => @origine } %>

<p style="color: green"><%= flash[:notice] %> </p>

<table width="100%" cellspacing="0" > 
<% odd_or_even = 0 %>
<% for protref in @protrefs
odd_or_even = 1 - odd_or_even %>
<tr class="item<%= odd_or_even %>">
	<td style="border-top:1px solid blue;">
					  <p class="exp_index_normal" > <b> <%= h protref.title %> </b> </p>
	</td>                 
	<td style="border-top:1px solid blue;">           
	                  <p align="right" class="exp_index_link"> <%= link_to 'Details', {:action => 'detail_from_index', :id => protref} , {:style => 'color:blue;'} %> - 
	                  <%= link_to 'Edit', {:action => 'edit_from_index', :id=> protref}, {:style => 'color:blue;'} %>
	</td>
</tr>
<%- end %>
</table>


<br />

<%= render :partial => "headfoot", :locals => { :origin => @origine } %>

